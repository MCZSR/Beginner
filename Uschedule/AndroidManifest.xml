<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uschedule"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.uschedule.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.uschedule.activity.LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustResize|stateVisible" >
        </activity>
        <activity
            android:name="com.example.uschedule.MainActivity2"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name="com.example.uschedule.Am"
            android:label="@string/title_activity_am" >
        </activity>
        <activity
            android:name="com.example.uschedule.Bm"
            android:label="@string/title_activity_bm" >
        </activity>
        <activity
            android:name="com.example.uschedule.activity.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.example.uschedule.Interim"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.mc.activity.ScheduleView"
            android:label="@string/app_name" />
        <activity
            android:name="com.mc.activity.ScheduleTypeView"
            android:label="@string/app_name" />
        <activity
            android:name="com.mc.activity.ScheduleInfoView"
            android:label="@string/app_name" />
        <activity
            android:name="com.mc.activity.ScheduleAll"
            android:label="@string/app_name" />
        <activity
            android:name="com.mc.activity.CalendarConvert"
            android:label="@string/app_name" />
    </application>

</manifest>
